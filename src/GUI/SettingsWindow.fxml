<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.SettingsWindow">
    <children>
        <BorderPane>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <opaqueInsets>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </opaqueInsets>
            <top>
                <HBox alignment="TOP_RIGHT" BorderPane.alignment="CENTER">
                    <children>
                        <ImageView fitHeight="150.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@pic/MSC.PNG"/>
                            </image>
                        </ImageView>
                    </children>
                </HBox>
            </top>
            <center>
                <VBox alignment="TOP_CENTER" BorderPane.alignment="TOP_CENTER">
                    <children>
                        <Label alignment="TOP_CENTER" text="הגדרות">
                            <font>
                                <Font size="30.0"/>
                            </font>
                        </Label>
                    </children>
                </VBox>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
